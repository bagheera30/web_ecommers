---

import { Image } from 'astro:assets';
import { Icon } from 'astro-icon'
import Foot from "../components/header/Foot.astro";
import Layouts from '../layouts/Layouts.astro';
const title="Home";

---


<Layouts title={title}>
		<div class="bg-gray-900 h-auto">
			<!-- navbar -->
			<div class="navbar bg-gray-900 bg-opacity-9 fixed">
				<div class="flex-1">
				  <a href="/" class="btn btn-ghost normal-case text-3xl text-white ">MultiSellerSquare</a>
				</div>
				<div class="flex-none gap-2">
					<div class="  flex justify-between">
						<a href="/dasbord"><button class="btn rounded-3xl mx-2 w-32 h-10">Dasbord</button></a>
						<a href="/Products"><button class="btn rounded-3xl mx-2 w-32 h-10">Products</button></a>
						<button class="btn rounded-3xl mx-2 w-32 h-10">Contact Us</button>
					  </div>
					  
				  <div class="dropdown dropdown-end">
					<label tabindex="0" class="btn btn-ghost btn-circle avatar">
						<div class="w-24 rounded-full">
							<Icon name="mdi:account-outline" class="text-[#815A2C]"/>
						  </div>
					</label>
					<ul tabindex="0" class="mt-3 z-[1] p-2 shadow menu menu-sm dropdown-content bg-base-100 rounded-box w-52">
					  <li>
						<a class="justify-between" href="/login">
						  login
						  
						</a>
					  </li>
					  <li><a href="/regiter">Register</a></li>
					</ul>
				  </div>
				</div>
			</div>
			<div class="px-32 py-32 justify-between">
				<h2 class="text-8xl text-red-700 font-normal font-['Italianno'] my-5">E-commerce</h2>
				<h3 class="text-6xl text-white font-normal font-['Italianno'] py-5">Search.</h3>
				<h3 class="text-6xl text-white font-normal font-['Italianno'] py-5">Buy. Arrived.</h3>
				<img class="w-14 h-20 origin-top-left rotate-[103.46deg]" src="https://via.placeholder.com/102x122" />
				<p class="text-white text-2xl font-normal font-['Roboto'] py-10">Multi Seller Square adalah website <br/>e-commerce yang dirancang untuk mempermudah anda mencari produk apa saja yang anda inginkan.</p>
			</div>
		</div>
		<Foot/>
</Layouts>
